
<!DOCTYPE html>
<html lang="en">
  <head>

    {% if title %}
      <title>{{ title }}</title>
    {% else %}
      <title>SDE Diary</title>
    {% endif %}

    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    {% block metatags %}
    {% endblock metatags %}

    <!-- Bootstrap 5 CSS only -->
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-eOJMYsd53ii+scO/bJGFsiCZc+5NDVN2yr8+0RDqr0Ql0h+rP48ckxlpbzKgwra6"
      crossorigin="anonymous"
    />

    <!-- Google fonts -->
    <link rel="preconnect" href="https://fonts.gstatic.com" />

    <!-- Ubuntu Fonts from google fonts-->
    <link
      href="https://fonts.googleapis.com/css2?family=Ubuntu:ital,wght@0,300;0,400;0,500;0,700;1,300;1,400;1,500;1,700&display=swap"
      rel="stylesheet"
    />

    <!-- Montserrat Fonts from google fonts-->
    <link
      href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&family=Ubuntu:ital,wght@0,300;0,400;0,500;0,700;1,300;1,400;1,500;1,700&display=swap"
      rel="stylesheet"
    />

    <!-- Animate.css style sheets -->
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css"
    />

    <!-- Jquery from google -->
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>

    <link rel="icon" href="/static/assets/icon.svg" />
    <link rel="stylesheet" href="/static/styles/diary/index.css">

    {% block headers %}
    {% endblock headers %}
  </head>
  <body>

    <section id="diary">
        <div class="container-fluid">
            <div class="row">
                <div class="col-xl-6 calendar-container">
                    <div class="calendar">Test</div>
                </div>
                <div class="col-xl-6 viewer-container">
                    <div class="viewer">Test</div>
                </div>
                <div class="col-xl-6 entry-container">
                    <div class="entry">Test</div>
                </div>
            </div>
        </div>
    </section>

    <!-- Font Awesome Icons -->
    <script
      src="https://kit.fontawesome.com/7a4bf45098.js"
      crossorigin="anonymous"
    ></script>

    <!-- Bootstrap 5 JavaScript Bundle with Popper -->
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-JEW9xMcG8R+pH31jmWH6WWP0WintQrMb4s7ZOdauHnUtxwoG2vI5DkLtS3qm9Ekf"
      crossorigin="anonymous"
    ></script>


    {% block js %}
    {% endblock js %}
  </body>
</html>
